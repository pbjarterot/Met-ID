{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "externalBin": [
      "pyinstaller/dist/metabolite",
      "pyinstaller/dist/metabolite_for_db"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "resources": [
      "DBs/*",
      "pyinstaller/dist/*"
    ]
  },
  "productName": "Met-ID",
  "mainBinaryName": "Met-ID",
  "version": "0.5.7",
  "identifier": "com.farmbio.metid",
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "passive"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDM1MUQxQUVFQkZFQ0VBNDQKUldSRTZ1eS83aG9kTldBaG1hQWY5Q0g4NCtWZDlyY0xKcWd4VjFPTjFNaEIzUmVKTEVFTE9TQTAK",
      "endpoints": [
        "https://github.com/pbjarterot/Met-ID/releases/latest/download/latest.json"
      ]
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "met-id",
        "width": 1900,
        "height": 1000,
        "decorations": false,
        "useHttpsScheme": true
      },
      {
        "width": 310,
        "height": 310,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "transparent": true,
        "useHttpsScheme": true
      }
    ],
    "withGlobalTauri": true,
    "trayIcon": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "security": {
      "csp": null
    }
  }
}